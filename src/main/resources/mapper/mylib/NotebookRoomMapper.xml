<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  

<mapper namespace="com.library.mapper.mylib.NotebookRoomMapper">

	<!-- 전체 좌석 조회 -->
	<select id="nb_list_all" resultType="com.library.model.mylib.NoteBookRoomDTO">
		select *
		  from reading_room where seat_no &gt; 96
		 order by seat_no
	</select>










	<!-- 좌석 insert(test에서 진행) -->
	<insert id="insert">
		insert into reading_room values(#{seat_no}, null, null, null);
	</insert>
</mapper>