<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.library.mapper.board.CalendarMapper">

	<resultMap type="com.library.model.board.CalendarDTO" id="calMap">
		<result property="start" column="start1" />
		<result property="end" column="end1" />
	</resultMap>
	<select id="list_all" resultMap="calMap"> 
		SELECT * FROM CALENDAR 
	</select>
	
	<insert id="insert_cal">
		insert into calendar values(null, 1, #{user_id}, #{start1}, #{end1}, 1, "black", "white", "white");
	</insert>
</mapper>
